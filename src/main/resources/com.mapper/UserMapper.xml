<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.Usermapper">
    <insert id="register" parameterType="com.pojo.User">
        insert into user1 (username,password) values(#{username},#{password})
    </insert>
    <select id="selectUserByName" parameterType="String"
            resultType="com.pojo.User">
        select * from user1 where username = #{username}
    </select>
</mapper>